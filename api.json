{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "allOf": [
    { "$ref": "#/$defs/Data" },
    { "$ref": "#/$defs/SelfLink" },
    { "$ref": "#/$defs/CollectionLink"}
  ],
  "$defs": {
    "Data": {
      "type": "object",
      "properties": {
        "data": {
          "type": "string"
        }
      }
    },
    "SelfLink": {
      "type": "object",
      "properties": {
        "links": {
          "type": "object",
          "properties": {
            "self": {
              "type": "string"
            }
          }
        }
      }
    },
    "CollectionLink": {
      "type": "object",
      "properties": {
        "links": {
          "type": "object",
          "properties": {
            "collection": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
